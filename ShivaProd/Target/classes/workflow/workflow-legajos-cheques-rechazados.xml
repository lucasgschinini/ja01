  	<util:constant id="lgj_anulado"											static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_ANULADO"/>
  	<util:constant id="lgj_desistido"										static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_DESISTIDO"/>
