  	<util:constant id="lgj_no_disponible"									static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_NO_DISPONIBLE"/>
  	<util:constant id="lgj_abierto"											static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_ABIERTO"/>
  	<util:constant id="lgj_en_proceso_reversion_cobros_relacionados"		static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_EN_PROCESO_REVERSION_COBROS_RELACIONADOS"/>
  	<util:constant id="lgj_en_proceso_reembolso_con_cheque_pend_entrega"	static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_EN_PROCESO_REEMBOLSO_CON_CHEQUE_PEND_ENTREGA"/>
