  		<property name="destino" ref="lgj_en_proceso_reembolso_con_cheque_entregado"/>	
  	</bean>

	<bean id="solicitarEnvioALegalesParaLegajoEnProcesoDeReembolso" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
