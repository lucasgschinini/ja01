  		<property name="origen" ref="lgj_cerrado_con_cheque_pend_entrega"/>
  		<property name="destino" ref="lgj_cerrado_con_cheque_entregado"/>	
  	</bean>

	<bean id="solicitarEntregaDeChequeRechazadoParaLegajoReembolsado" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Entrega de Cheque para Legajo Reembolsado"/>
  		<property name="evento" value="solicitarEntregaDeChequeRechazadoParaLegajoReembolsado"/>
  		<property name="origen" ref="lgj_reembolsado_con_cheque_pend_entrega"/>
  		<property name="destino" ref="lgj_reembolsado_con_cheque_entregado"/>	
  	</bean>

	<bean id="solicitarFinalizarProcesoDeReembolsoParaLegajoConChequeRechazadoEntregado" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Finalizar Proceso de Reembolso para Legajo con Cheque Rechazado Entregado"/>
  		<property name="evento" value="solicitarFinalizarProcesoDeReembolsoParaLegajoConChequeRechazadoEntregado"/>
  		<property name="origen" ref="lgj_en_proceso_reembolso_con_cheque_entregado"/>
  		<property name="destino" ref="lgj_reembolsado_con_cheque_entregado"/>	
