  		<property name="evento" value="solicitarCierreParaLegajoConChequeRechazadoEntregado"/>
  		<property name="origen" ref="lgj_reembolsado_con_cheque_entregado"/>
  		<property name="destino" ref="lgj_cerrado_con_cheque_entregado"/>	
  	</bean>

	<bean id="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoAbierto" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Cierre con Cheque Rechazado Pendiente de Entrega para Legajo Abierto"/>
  		<property name="evento" value="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoAbierto"/>
