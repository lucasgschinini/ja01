  		<property name="destino" ref="lgj_cerrado_con_cheque_pend_entrega"/>	
  	</bean>

	<bean id="solicitarEntregaDeChequeRechazadoParaLegajoAbierto" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Entrega de Cheque para Legajo Abierto"/>
  		<property name="evento" value="solicitarEntregaDeChequeRechazadoParaLegajoAbierto"/>
