  	<util:constant id="lgj_reembolsado_con_cheque_pend_entrega"				static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.LGJ_REEMBOLSADO_CON_CHEQUE_PEND_ENTREGA"/>
