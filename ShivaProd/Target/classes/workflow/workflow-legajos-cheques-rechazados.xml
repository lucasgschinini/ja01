<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	
  	<bean id="LEGAJOS_CHEQUES_RECHAZADOS" class="ar.com.telecom.shiva.negocio.workflow.definicion.Workflow">
  		<property name="tipoWorkflow">
  			<util:constant static-field="ar.com.telecom.shiva.negocio.workflow.definicion.TipoWorkflow.LEGAJOS_CHEQUES_RECHAZADOS" />
 		</property>
 		
  		<property name="transiciones">
  			<list>
  				<ref bean="inicioLegajo"/>
  				
				<ref bean="solicitarAperturaDeLegajo"/>
				<ref bean="anularLegajoAbierto"/>
				<ref bean="solicitarReversionDeCobrosrelacionados"/>
				<ref bean="solicitarOmitirProcesoDeReembolsoParaLegajoConChequeRechazadoAbierto"/>
				<ref bean="anularLegajoEnProcesoDeReversion"/>
				<ref bean="solicitarInicioProcesodeReembolso"/>
				<ref bean="solicitarFinalizarProcesodeReembolsoParaLegajoConChequeRechazadoPendienteDeEntrega"/>
				<ref bean="solicitarEntregaDeChequeRechazadoParaLegajoEnProcesoDeReembolso"/>
				<ref bean="solicitarEnvioALegalesParaLegajoEnProcesoDeReembolso"/>
				<ref bean="solicitarDesistir"/>
				<ref bean="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoReembolsado"/>
				<ref bean="solicitarEntregaDeChequeRechazadoParaLegajoCerrado"/>
				<ref bean="solicitarEntregaDeChequeRechazadoParaLegajoReembolsado"/>
				<ref bean="solicitarFinalizarProcesoDeReembolsoParaLegajoConChequeRechazadoEntregado"/>
				<ref bean="solicitarCierreParaLegajoConChequeRechazadoEntregado"/>
				<ref bean="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoAbierto"/>
				<ref bean="solicitarEntregaDeChequeRechazadoParaLegajoAbierto"/>
				<ref bean="solicitarCierreDeLegajoParaChequeRechazadoEntregado"/>
				<ref bean="solicitarEnvioALegalesParaLegajoAbierto"/>
				<ref bean="solicitarOmitirProcesoDeReembolsoParaLegajoConChequeRechazadoPendienteDeEntrega"/>
  			</list>
  		</property>	
  	</bean>
  	
