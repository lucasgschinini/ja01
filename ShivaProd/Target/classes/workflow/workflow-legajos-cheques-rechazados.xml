	<bean id="solicitarInicioProcesodeReembolso" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Inicio Proceso de Reembolso"/>
  		<property name="evento" value="solicitarInicioProcesodeReembolso"/>
  		<property name="origen" ref="lgj_en_proceso_reversion_cobros_relacionados"/>
  		<property name="destino" ref="lgj_en_proceso_reembolso_con_cheque_pend_entrega"/>	
