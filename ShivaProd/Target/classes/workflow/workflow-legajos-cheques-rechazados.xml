	<bean id="solicitarDesistir" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Desistir"/>
  		<property name="evento" value="solicitarDesistir"/>
  		<property name="origen" ref="lgj_enviado_a_legales"/>
  		<property name="destino" ref="lgj_desistido"/>	
  	</bean>

	<bean id="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoReembolsado" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Cierre con Cheque Rechazado Pendiente de Entrega para Legajo Reembolsado"/>
  		<property name="evento" value="solicitarCierreConChequeRechazadoPendienteDeEntregaParaLegajoReembolsado"/>
  		<property name="origen" ref="lgj_reembolsado_con_cheque_pend_entrega"/>
  		<property name="destino" ref="lgj_cerrado_con_cheque_pend_entrega"/>	
  	</bean>

	<bean id="solicitarEntregaDeChequeRechazadoParaLegajoCerrado" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Entrega de Cheque para Legajo Cerrado"/>
