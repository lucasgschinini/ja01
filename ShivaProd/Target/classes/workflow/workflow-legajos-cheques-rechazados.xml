	<bean id="solicitarFinalizarProcesodeReembolsoParaLegajoConChequeRechazadoPendienteDeEntrega" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Finalizar Proceso de Reembolso para Legajo con Cheque Rechazado Pendiente de Entrega"/>
  		<property name="evento" value="solicitarFinalizarProcesodeReembolsoParaLegajoConChequeRechazadoPendienteDeEntrega"/>
  		<property name="origen" ref="lgj_en_proceso_reembolso_con_cheque_pend_entrega"/>
  		<property name="destino" ref="lgj_reembolsado_con_cheque_pend_entrega"/>	
