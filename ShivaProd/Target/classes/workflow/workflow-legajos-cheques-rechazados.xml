  		<property name="destino" ref="lgj_cerrado_con_cheque_entregado"/>	
  	</bean>

	<bean id="solicitarEnvioALegalesParaLegajoAbierto" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">  		
		<property name="nombre" value="Solicitar Envío a Legales para Legajo Abierto"/>
  		<property name="evento" value="solicitarEnvioALegalesParaLegajoAbierto"/>
  		<property name="origen" ref="lgj_abierto"/>
  		<property name="destino" ref="lgj_enviado_a_legales"/>	
  	</bean>
  	
	<bean id="solicitarOmitirProcesoDeReembolsoParaLegajoConChequeRechazadoPendienteDeEntrega" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">  		
