  		<property name="nombre" value="Inicio"/>
  		<property name="evento" value="crearWorkflow"/>
  		<property name="origen">
  			<util:constant static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.INICIO" />
  		</property>
  		<property name="destino" ref="lgj_no_disponible"/>	
  	</bean>

	<bean id="solicitarAperturaDeLegajo" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Apertura de Legajo"/>
  		<property name="evento" value="solicitarAperturaDeLegajo"/>
  		<property name="origen" ref="lgj_no_disponible"/>
  		<property name="destino" ref="lgj_abierto"/>	
  	</bean>

	<bean id="anularLegajoAbierto" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Anular Legajo Abierto"/>
  		<property name="evento" value="anularLegajoAbierto"/>
  		<property name="origen" ref="lgj_abierto"/>
  		<property name="destino" ref="lgj_anulado"/>	
  	</bean>

	<bean id="solicitarReversionDeCobrosrelacionados" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
		<property name="nombre" value="Solicitar Reversión de cobros relacionados"/>
  		<property name="evento" value="solicitarReversionDeCobrosrelacionados"/>
  		<property name="origen" ref="lgj_abierto"/>
  		<property name="destino" ref="lgj_en_proceso_reversion_cobros_relacionados"/>	
  	</bean>

	<bean id="solicitarOmitirProcesoDeReembolsoParaLegajoConChequeRechazadoAbierto" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
