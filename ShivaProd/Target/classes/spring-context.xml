<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="props" class="ar.com.telecom.shiva.base.utils.UtilPropertiesHolder">
  		<property name="locations">
  			<list>
  				<value>classpath:shiva_${entorno}_interno.properties</value>
  				<value>file:${configuracion}</value>
  			</list>
  		</property>
	</bean>
	
   	<import resource="context/spring-webmvc.xml" />
   	<import resource="context/spring-crypto.xml" />
	<import resource="context/spring-jdbc-x-war.xml" />
	<import resource="context/spring-jms-base.xml" />
	<import resource="context/spring-captcha.xml" />
	<import resource="context/spring-security-x-war.xml" />
	<import resource="context/spring-ws.xml" />
	<import resource="context/spring-scheduler-task.xml" />
	<import resource="context/spring-aop.xml" />
	<import resource="context/spring-registros-mapeos.xml" />
	<import resource="context/spring-context-reports.xml" />
	
</beans>