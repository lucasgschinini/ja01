<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:jms="http://www.springframework.org/schema/jms"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
                           http://www.springframework.org/schema/jms http://www.springframework.org/schema/jms/spring-jms.xsd">
    
   	<context:component-scan base-package="ar.com.telecom.shiva.base.jms" />
    
    <import resource="spring-jms-template.xml" />
    <import resource="spring-jms-servicios.xml" />
    <import resource="spring-jms-mapeos.xml" />
    
    <!-- 
    ***************************************************** 
    Asincronicas
    *****************************************************
    -->
    <!-- Mic: Imputacion Cobros (Asincronico)  -->
    <!-- Mic: Generacion Cargo (Asincronico) -->
    <bean id="micImputacionCobrosJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.fac.aplicacion.async}" />
		<property name="servicio" value="" />
	</bean>
    
    <bean id="micImputacionCobrosMQSeries" class="ar.com.telecom.shiva.base.jms.util.mq.MQSeriesBase">
    	<property name="connectionFactory" ref="micFacJmsConnectionFactory"/>
    	<property name="queueEmisor" ref="micFacEnvioJmsQueue"/>
		<property name="queueReceptor" ref="micFacRecepcionJmsQueue"/>
		<property name="autenticacion" ref="micFacJmsAutenticacion"/>
    </bean>
    
    <bean id="micImputacionCobrosControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micImputacionCobrosMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micImputacionCobrosJmsPropiedades"/>
    </bean>
    
    <bean id="micImputacionCobrosTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micImputacionCobrosMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micImputacionCobrosJmsPropiedades"/>
    </bean>
    
    <!-- Mic: Imputacion Descobros (Asincronico)  -->
    <bean id="micImputacionDescobrosJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.imputacion.descobro.aplicacion.async}" />
		<property name="servicio" value="" />
	</bean>
    
    <bean id="micImputacionDescobrosMQSeries" class="ar.com.telecom.shiva.base.jms.util.mq.MQSeriesBase">
    	<property name="connectionFactory" ref="micFacJmsConnectionFactory"/>
    	<property name="queueEmisor" ref="micFacEnvioJmsQueue"/>
		<property name="queueReceptor" ref="micImputacionDescobroRecepcionJmsQueue"/>
		<property name="autenticacion" ref="micFacJmsAutenticacion"/>
    </bean>
    
    <bean id="micImputacionDescobrosControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micImputacionDescobrosMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micImputacionDescobrosJmsPropiedades"/>
    </bean>
    
    <bean id="micImputacionDescobrosTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micImputacionDescobrosMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micImputacionDescobrosJmsPropiedades"/>
    </bean>
    
    <!-- 
    ***************************************************** 
    Sincronicas
    *****************************************************
    -->
    
    <!-- Sprint 5 -->
    <!-- Mic: Simulacion Cobros (Sincronico) -->
    <!-- Mic: Simulacion Generacion Cargo (Sincronico) -->
    <!-- Mic: Simulacion Descobros (Sincronico) -->
    <bean id="micSimulacionCobrosJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.fac.aplicacion.sync}" />
		<property name="servicio" value="" />
	</bean>
	 
    <bean id="micSimulacionCobrosControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micSimulacionCobrosJmsPropiedades"/>
    </bean>
    
    <bean id="micSimulacionCobrosTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micSimulacionCobrosJmsPropiedades"/>
    </bean>
   
   	<!-- 
    ***************************************************** 
    Sincronicas
    *****************************************************
    --> 
    <!-- MQ Series (Sincronico) para Mic/Fac -->
    <bean id="micFacSincronicoMQSeries" class="ar.com.telecom.shiva.base.jms.util.mq.MQSeriesBase">
    	<property name="connectionFactory" ref="micFacJmsConnectionFactory"/>
    	<property name="queueEmisor" ref="micFacEnvioJmsQueue"/>
		<property name="queueReceptor" ref="micFacRecepcionJmsSyncQueue"/>
		<property name="autenticacion" ref="micFacJmsAutenticacion"/>
    </bean>
    
    <!-- Fac: Consultas Acuerdo, Cliente, Linea (Sincronico) -->
    <bean id="facConsultasJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.fac.aplicacion.sync}" />
		<property name="servicio" value="${mq.fac.consultas.servicio}" />
	</bean>
	
    <bean id="facConsultasControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="facConsultasJmsPropiedades"/>
    </bean>
    
    <bean id="facConsultasTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="facConsultasJmsPropiedades"/>
    </bean>
    
    <!-- Mic: Consulta Credito (Sincronico) -->
    <bean id="micConsultaCreditoJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.fac.aplicacion.sync}" />
		<property name="servicio" value="${mq.mic.consulta.credito.servicio}" />
	</bean>
	 
    <bean id="micConsultaCreditoControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micConsultaCreditoJmsPropiedades"/>
    </bean>
    
    <bean id="micConsultaCreditoTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micConsultaCreditoJmsPropiedades"/>
    </bean>
    
    <!-- Mic: Consulta Deuda (Sincronico) -->
    <bean id="micConsultaDeudaJmsPropiedades" class="ar.com.telecom.shiva.base.jms.util.JmsPropiedades">
		<property name="aplicacion" value="${mq.mic.fac.aplicacion.sync}" />
		<property name="servicio" value="${mq.mic.consulta.deuda.servicio}" />
	</bean>
	 
    <bean id="micConsultaDeudaControlMQ" class="ar.com.telecom.shiva.base.jms.impl.ControlMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micConsultaDeudaJmsPropiedades"/>
    </bean>
    
    <bean id="micConsultaDeudaTesterMQ" class="ar.com.telecom.shiva.base.jms.impl.TesterMqSeriesImpl">
    	<property name="mqServidor" ref="micFacSincronicoMQSeries"/>
    	<property name="jmsTemplate" ref="micFacJmsTemplate"/>
    	<property name="jmsPropiedades" ref="micConsultaDeudaJmsPropiedades"/>
    </bean>
    
</beans>