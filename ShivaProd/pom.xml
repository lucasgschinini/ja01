<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>shiva</groupId>
  <artifactId>shiva</artifactId>
  <version>${fecha.version}-${appl.version}</version>
  <packaging>war</packaging>
  
  <properties>
 		<!-- Versiones propias -->
 		<fecha.version>20190430</fecha.version>
 		<nro.version>4.10.05</nro.version>
 		<num.entrega>.02</num.entrega>
 		
 		<num.version>v${nro.version}</num.version>
 		<appl.version>${num.version}${num.entrega}</appl.version>
 		<entrega.version>Version ${nro.version}</entrega.version>
 		
 		<deliveryFolder>${project.artifactId}-${project.version}</deliveryFolder>
 		
 		<!-- Versiones de Librerias -->
		<spring-version>3.2.6.RELEASE</spring-version>
		<spring-security-version>3.2.0.RELEASE</spring-security-version>
		<spring-batch-version>2.2.6.RELEASE</spring-batch-version>
		<slf4j-version>1.7.5</slf4j-version>
		<logback-version>1.0.13</logback-version>
		<drools-version>5.5.0.Final</drools-version>
		<jbpm-version>5.4.0.Final</jbpm-version>
		<websphere-mq-version>7.5.0.0</websphere-mq-version>
		<metro-version>2.2.8</metro-version>
		<cxf-version>2.7.6</cxf-version>
	</properties>
  
  	<build>
		<directory>Target</directory>
		<sourceDirectory>src/main/java</sourceDirectory>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
		</resources>
		<outputDirectory>Target/classes</outputDirectory>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.7</version>
				<executions>
					<execution>
						<id>1: Preparar fuentes</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>src/main/java</source>
								<source>src/main/resources</source>
							</sources>
		              		<encoding>Cp1252</encoding>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<!-- CXF -->
			<plugin>
				<groupId>org.apache.cxf</groupId>
				<artifactId>cxf-codegen-plugin</artifactId>
				<version>${cxf-version}</version>
				<executions>
					<execution>
						<id>2: Generar fuentes para WS</id>
						<phase>generate-sources</phase>
						<configuration>
							<defaultOptions>
							   	<bindingFiles>
									<bindingFile>${basedir}/src/main/resources/wsdl/binding.xml</bindingFile>
								</bindingFiles>
								<noAddressBinding>true</noAddressBinding>
							</defaultOptions>
							<sourceRoot>${basedir}/src/main/java</sourceRoot>
							<wsdlOptions>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/siebel/ClienteConsultarContract.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.siebel</extraarg>
										<extraarg>-b</extraarg>
										<extraarg>${basedir}/src/main/resources/wsdl/siebel/messageHeaderBinding.xml</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/delta/WebServiceTeamComercialCliente.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.delta</extraarg>
										<extraarg>-b</extraarg>
										<extraarg>${basedir}/src/main/resources/wsdl/delta/messageHeaderBinding.xml</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoCobranzas.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.cobranzas</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoConsultaAcuerdo.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.consultaAcuerdo</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoCargos.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.cargos</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoConsultaDeudayCredito.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.consultadeudaycredito</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/deimos/WebServiceDeimos.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.deimos</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceReversionCobros.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.descobros</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/sap/ZFI_SHIVA_VENDOR_FIND_WS.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.sap.s4.consultaProveedores</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/sap/r3/ZFI_SHIVA_VENDOR_FIND_WS.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.sap.r3.consultaProveedores</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/sap/ZFI_SHIVA_VERIF_PARTIDAS_WS.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.sap.verificacionPartidas</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/sap/ZFI_SHIVA_GETOPENITEMS_WS.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.sap.consultaPartidas</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/sap/ZFI_SHIVA_POST_COMPENSACION_WS.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.sap.imputacion</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/ice/ConsultaCheques.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.ice.consultaCheques</extraarg>
									</extraargs>
								</wsdlOption>
							</wsdlOptions>
		              		<encoding>Cp1252</encoding>
						</configuration>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.0</version>
				<executions>
					<execution>
						<id>3: Compilacion</id>
						<phase>compile</phase>
					</execution>
				</executions>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<compilerArgument>-XDignore.symbol.file</compilerArgument>
              		<encoding>Cp1252</encoding>
				</configuration>
			</plugin>
			<!-- 
			Permite ejecutar las pruebas unitarias (Test)
			En este caso (por ser skipTests=true) no voy a ejecutar ninguna prueba 
			y se saltea 
			-->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.12.4</version>
				<executions>
					<execution>
	               		<id>4: Pruebas unitarias</id>
	                  	<phase>test</phase>
	                </execution> 
				</executions>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>5: Preparar Jar para el Batch</id>
	                  	<phase>process-classes</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
		                  	<tasks>
	                 			<echo>Creo la carpeta para contener el JAR...</echo>
	                  			<mkdir dir="${project.build.directory}/entrega/${deliveryFolder}/batch"/>
	
		                 		<echo>Preparando el JAR para el Batch</echo>
								<echo>Se copia la configuracion de log para los batch</echo>
								<delete file="src/main/resources/logback.xml" />
	               				<delete file="${project.build.outputDirectory}/logback.xml" />
	                   			<copy file="src/main/resources/backup/logback/logback-batch.xml" tofile="src/main/resources/logback.xml"/>
	                   			<copy file="src/main/resources/backup/logback/logback-batch.xml" tofile="${project.build.outputDirectory}/logback.xml"/>
								<echo>Se borra la version anterior</echo>
			             		<delete dir="${project.build.directory}/entrega/${deliveryFolder}"/>
		                  	</tasks>
		              		<encoding>Cp1252</encoding>
	                  	</configuration>
	               	</execution>
	            </executions>
	        </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.4</version>
				<executions>
					<execution>
						<id>6: Crear JAR para el Batch</id>
						<phase>process-classes</phase>
						<goals>
							<goal>jar</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/entrega/${deliveryFolder}/batch/</outputDirectory>
							<finalName>${deliveryFolder}-Batch</finalName>
							<excludes>
		                		<exclude>**/spring-context.xml</exclude>
		                		<exclude>**/spring-jdbc-x-war*.xml</exclude>
		                		<exclude>**/spring-security-x-war*.xml</exclude>
		                		<exclude>**/spring-captcha*.xml</exclude>
		                		<exclude>**/shiva*.bat</exclude>
		                		<exclude>**/shiva*.sh</exclude>
		                		<exclude>**/logback-*.xml</exclude>
		                		<exclude>**/definicionProcesamientoArchivoAvcAcuerdo.*.xml</exclude>
		              		</excludes>
		              		<encoding>Cp1252</encoding>
						</configuration>
					</execution>
        		</executions>	
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>9: Preparar WAR</id>
	                  	<phase>prepare-package</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  		<tasks>
	                  			
	                  			<echo>Creo la carpeta para contener el WAR...</echo>
	                  			<mkdir dir="${project.build.directory}/entrega/${deliveryFolder}/war"/>
	                  			
	                  			<echo>Creo las carpetas para los fuentes...</echo>
	                  			<mkdir dir="${project.build.directory}/entrega/${deliveryFolder}/fuentes"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${deliveryFolder}/fuentes/Shiva"/>
	                  			
	                  			<echo>Copiando carpeta src...</echo>
	                  			<copy todir="${project.build.directory}/entrega/${deliveryFolder}/fuentes/Shiva/src/">
    								<fileset dir="src">
        								<include name="**/*.*"/>  
    								</fileset>
								</copy>
								
								<echo>Copiando carpeta WebContent...</echo>
	                  			<copy todir="${project.build.directory}/entrega/${deliveryFolder}/fuentes/Shiva/WebContent/">
    								<fileset dir="WebContent">
        								<include name="**/*.*"/>  
    								</fileset>
								</copy>
								
								<echo>Copiando pom...</echo>
	                  			<copy todir="${project.build.directory}/entrega/${deliveryFolder}/fuentes/Shiva/">
    								<fileset dir="">
        								<include name="pom.xml"/>  
    								</fileset>
								</copy>
								<echo>Se copia la configuracion de log para el war</echo>
								<delete file="src/main/resources/logback.xml" />
			                 			<delete file="${project.build.outputDirectory}/logback.xml" />
	                 			
			                 			<copy file="src/main/resources/backup/logback/logback-jboss.xml" 
			                     	   		tofile="src/main/resources/logback.xml"/>
			                     	   		<copy file="src/main/resources/backup/logback/logback-jboss.xml" 
			                     	   		tofile="${project.build.outputDirectory}/logback.xml"/>

								<echo>Zipeando Proyecto...</echo>
								<zip destfile="${project.build.directory}/entrega/${deliveryFolder}/fuentes/shiva-sources.zip">
									<zipfileset dir="${project.basedir}/src" prefix="src" />
									<zipfileset dir="${project.basedir}/WebContent" prefix="WebContent" excludes="WEB-INF/lib/**"/>
									<zipfileset dir="." includes="pom.xml,.project,.classpath"/>
								</zip>
	                  		</tasks>
		              		<encoding>Cp1252</encoding>
	                  	</configuration>
	               </execution>
	            </executions>
	        </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.3</version>
				<executions>
					<execution>
						<id>12: Crear WAR</id>
						<phase>package</phase>
					</execution>
				</executions>
				<configuration>
					<outputDirectory>${project.build.directory}/entrega/${deliveryFolder}/war/</outputDirectory>
					<warName>${deliveryFolder}</warName>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<failOnMissingWebXml>false</failOnMissingWebXml>
					<packagingExcludes>
						**/spring-context-batch.xml,
						**/spring-jdbc-x-jar*.xml,
						**/spring-security-x-jar*.xml,
						**/spring-context-batch*.xml,
						**/logback-*.xml,
						**/shiva*.bat,
						**/shiva*.sh
					</packagingExcludes>
              		<encoding>Cp1252</encoding>
				</configuration>
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>13: Armando Zip con WAR y JAR para Jenkins</id>
	                  	<phase>install</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  		<tasks>
	                  			<!-- 
								<echo>Creo la carpeta para contener el 'empaquedato'...</echo>									
		                  		<mkdir dir="${project.build.directory}/entrega/${deliveryFolder}/packaging"/>
 								-->
								<echo>Creo el 'empaquetado'...</echo>
								<zip destfile="${project.build.directory}/shiva.zip">
									<zipfileset dir="${project.build.directory}/entrega/${deliveryFolder}/batch" includes="*.jar" fullpath="shiva-Batch.jar"/>
									<zipfileset dir="${project.build.directory}/entrega/${deliveryFolder}/war" includes="*.war" fullpath="Shiva.war"/>
								</zip>
	                  		</tasks>
		              		<encoding>Cp1252</encoding>
	                  	</configuration>
	               </execution>
	            </executions>
	        </plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>14: Ultimos</id>
	                  	<phase>install</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  		<tasks>
								<echo>Se borran los directorios generados automaticamente por maven install...</echo>
	                  			<delete dir="${project.build.directory}/${deliveryFolder}"/>
	                  			<delete dir="${user.home}/.m2/repository/shiva/shiva"/>
	                  			<delete dir="${project.build.directory}/entrega/${deliveryFolder}/fuentes/Shiva"/>
	                  		</tasks>
		              		<encoding>Cp1252</encoding>
	                  	</configuration>
	               </execution>
	            </executions>
	        </plugin>	        
		</plugins>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.apache.maven.plugins
										</groupId>
										<artifactId>
											maven-antrun-plugin
										</artifactId>
										<versionRange>
											[1.1,)
										</versionRange>
										<goals>
											<goal>run</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
  	</build>
	<repositories>
		<repository>
			<id>jboss</id>
			<url>http://repository.jboss.org/nexus/content/groups/public/</url>
		</repository>
		<repository>
			<id>redhat-techpreview-all-repository</id>
			<url>http://maven.repository.redhat.com/techpreview/all/</url>
		</repository>
	</repositories>
 
