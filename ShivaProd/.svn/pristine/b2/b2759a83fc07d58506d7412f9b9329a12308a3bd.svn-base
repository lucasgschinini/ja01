<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<!-- Servicios -->
	<bean id="jmsMonitorMensajeria" class="ar.com.telecom.shiva.base.jms.util.JmsMonitorMensajeria">
		<property name="micJmsServicio" ref="micJmsServicio"/>
		<property name="parametroServicio" ref="parametroServicio" />
		<property name="transaccionCobroServicio" ref="transaccionCobroServicio" />
	</bean>
	
	<bean id="micJmsServicio" class="ar.com.telecom.shiva.base.jms.servicios.impl.MicJmsServicioImpl">
		 <property name="micControlMQ" ref="micControlMQ" />
		 <property name="micADCMapeoJMS" ref="micADCMapeoJMS" />
		 <property name="micRespuestaADCMapeoJMS" ref="micRespuestaADCMapeoJMS" />
		 <property name="cobroServicio" ref="cobroServicio" />
		 <property name="reintentosMensajeria" ref="reintentosMensajeria" />
		 <property name="mensajeriaTransaccionServicio" ref="mensajeriaTransaccionServicio"/>
	</bean>
	
    <bean id="reintentosMensajeria" 
    	class="ar.com.telecom.shiva.base.soa.reintentos.ReintentosMensajeria">
		 <property name="mensajeriaTransaccionServicio" ref="mensajeriaTransaccionServicio"/>
		 <property name="parametroServicio" ref="parametroServicio" />
		 <property name="micControlMQ" ref="micControlMQ" />
	</bean>
    
</beans>