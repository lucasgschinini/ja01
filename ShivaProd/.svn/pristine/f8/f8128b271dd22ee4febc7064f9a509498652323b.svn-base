package ar.com.telecom.shiva.presentacion.bean.paginacion;

import java.math.BigInteger;

/**
 * @author u578936 MA.Uehara
 */
public class ConsultaCreditoCobrosPaginador {

	protected BigInteger cantidadDeRegistrosARetornar;
	private String idUltimoValor;
	private String ultimaFecha;
	private String idUltimoCliente;

	public ConsultaCreditoCobrosPaginador() {
	}

	public ConsultaCreditoCobrosPaginador(
		String idUltimoValor,
		String ultimaFecha,
		String idUltimoCliente
	) {
		super();
		this.idUltimoValor = idUltimoValor;
		this.ultimaFecha = ultimaFecha;
		this.idUltimoCliente = idUltimoCliente;
	}

	public String getIdUltimoValor() {
		return idUltimoValor;
	}
	public void setIdUltimoValor(String idUltimoValor) {
		this.idUltimoValor = idUltimoValor;
	}
	public String getUltimaFecha() {
		return ultimaFecha;
	}
	public void setUltimaFecha(String ultimaFecha) {
		this.ultimaFecha = ultimaFecha;
	}
	public String getIdUltimoCliente() {
		return idUltimoCliente;
	}
	public void setIdUltimoCliente(String idUltimoCliente) {
		this.idUltimoCliente = idUltimoCliente;
	}

	public BigInteger getCantidadDeRegistrosARetornar() {
		return cantidadDeRegistrosARetornar;
	}

	public void setCantidadDeRegistrosARetornar(
			BigInteger cantidadDeRegistrosARetornar) {
		this.cantidadDeRegistrosARetornar = cantidadDeRegistrosARetornar;
	}
}