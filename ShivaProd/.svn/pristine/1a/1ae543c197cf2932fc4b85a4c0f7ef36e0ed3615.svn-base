package ar.com.telecom.shiva.base.ws.cliente.datos.salida.agrupador.deimos;

import ar.com.telecom.shiva.base.enumeradores.SiNoEnum;

public class InformacionGeneralApropiacion {
	
	protected SiNoEnum apropiada;
	protected String   codigoEstadoTramite;
	protected String   descripcionEstadoTramite;
	protected Long convenio;
	protected Integer	cantidadCuotas;
	protected Integer	cantidadCuotasPagas;
	
	public SiNoEnum getApropiada() {
		return apropiada;
	}
	public void setApropiada(SiNoEnum apropiada) {
		this.apropiada = apropiada;
	}
	public String getCodigoEstadoTramite() {
		return codigoEstadoTramite;
	}
	public void setCodigoEstadoTramite(String codigoEstadoTramite) {
		this.codigoEstadoTramite = codigoEstadoTramite;
	}
	public String getDescripcionEstadoTramite() {
		return descripcionEstadoTramite;
	}
	public void setDescripcionEstadoTramite(String descripcionEstadoTramite) {
		this.descripcionEstadoTramite = descripcionEstadoTramite;
	}
	public Long getConvenio() {
		return convenio;
	}
	public void setConvenio(Long convenio) {
		this.convenio = convenio;
	}
	public Integer getCantidadCuotas() {
		return cantidadCuotas;
	}
	public void setCantidadCuotas(Integer cantidadCuotas) {
		this.cantidadCuotas = cantidadCuotas;
	}
	public Integer getCantidadCuotasPagas() {
		return cantidadCuotasPagas;
	}
	public void setCantidadCuotasPagas(Integer cantidadCuotasPagas) {
		this.cantidadCuotasPagas = cantidadCuotasPagas;
	}
	
	
	
}
