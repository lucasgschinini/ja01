package ar.com.telecom.shiva.presentacion.bean.filtro;


public class TalonarioFiltro extends Filtro {

	private static final long serialVersionUID = 1L;
	
	private String idEstado;
	private String rangoDesde;
	private String rangoHasta;
	private String nroSerie;
	private String observaciones;
	private String idTalonario;
	private String idTalonarioBandeja;


	public TalonarioFiltro(){}

	public TalonarioFiltro(String idEmpresa, String idSegmento) {
		super.setEmpresa(idEmpresa);
		super.setSegmento(idSegmento);
	}
	public String getIdTalonario() {
		return idTalonario;
	}
	
	public void setIdTalonario(String idTalonario) {
		this.idTalonario = idTalonario;
	}
	
	public String getObservaciones() {
		return observaciones;
	}

	public void setObservaciones(String observaciones) {
		this.observaciones = observaciones;
	}


	public String getIdEstado() {
		return idEstado;
	}
	public void setIdEstado(String idEstado) {
		this.idEstado = idEstado;
	}

	public String getRangoDesde() {
		return rangoDesde;
	}
	public void setRangoDesde(String rangoDesde) {
		this.rangoDesde = rangoDesde;
	}

	public String getRangoHasta() {
		return rangoHasta;
	}
	public void setRangoHasta(String rangoHasta) {
		this.rangoHasta = rangoHasta;
	}

	public String getNroSerie() {
		return nroSerie;
	}

	public void setNroSerie(String nroSerie) {
		this.nroSerie = nroSerie;
	}

	public String getIdTalonarioBandeja() {
		return idTalonarioBandeja;
	}

	public void setIdTalonarioBandeja(String idTalonarioBandeja) {
		this.idTalonarioBandeja = idTalonarioBandeja;
	}
}

