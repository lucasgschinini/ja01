<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cm="http://www.telecom.com.ar/XMLSchema/bios/common" xmlns:rqm="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage">
	<xs:import namespace="http://www.telecom.com.ar/XMLSchema/bios/common" schemaLocation="Diccionario.xsd"/>
	<xs:element name="resultadoActualizacionClienteReq">
		<xs:complexType>
			<xs:choice>
				<xs:element name="error">
					<xs:complexType>
						<xs:all>
							<xs:element ref="cm:errorCode"/>
							<xs:element ref="cm:descripcion" minOccurs="0"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="ok">
					<xs:complexType>
						<xs:all>
							<xs:element ref="cm:descripcion" minOccurs="0"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="legadoID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Codigo del Legado/Comercial (ebos) que invoca</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="legadoOfID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>CÃ³digo de Of. Comercial (ebos) de Legado</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="legadoClienteID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>ID del Cliente en Legado</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>