package ar.com.telecom.shiva.negocio.dto.cobros.imputacion;

public class ResultadoImputacionCobro {

	private int numHilo;
	private String idOperacion;
	private Boolean resultado = Boolean.TRUE;
	
	public ResultadoImputacionCobro(int numHilo, String idOperacion) {
		this.idOperacion = idOperacion;
		this.numHilo = numHilo;
	}
	
	public String getIdOperacion() {
		return idOperacion;
	}
	
	public void setIdOperacion(String idOperacion) {
		this.idOperacion = idOperacion;
	}

	public Boolean getResultado() {
		return resultado;
	}

	public void setResultado(Boolean resultado) {
		this.resultado = resultado;
	}

	public int getNumHilo() {
		return numHilo;
	}
}
