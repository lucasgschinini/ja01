package ar.com.telecom.shiva.persistencia.dao.impl;

import ar.com.telecom.shiva.persistencia.modelo.ShvMasOperacionMasiva;

public class OperacionMasivaVo {

	private ShvMasOperacionMasiva operacionMasiva;
	private long cantidadProcesado = 0;
	private long cantidadError = 0;
	
	public OperacionMasivaVo() {
	}
	public OperacionMasivaVo(ShvMasOperacionMasiva operacionMasiva, long cantidadProcesado, long cantidadError) {
		this.operacionMasiva = operacionMasiva;
		this.cantidadError = cantidadError;
		this.cantidadProcesado = cantidadProcesado;
	}
	

	/**
	 * @return the operacionMasiva
	 */
	public ShvMasOperacionMasiva getOperacionMasiva() {
		return operacionMasiva;
	}

	/**
	 * @param operacionMasiva the operacionMasiva to set
	 */
	public void setOperacionMasiva(ShvMasOperacionMasiva operacionMasiva) {
		this.operacionMasiva = operacionMasiva;
	}

	/**
	 * @return the cantidadProcesado
	 */
	public long getCantidadProcesado() {
		return cantidadProcesado;
	}

	/**
	 * @param cantidadProcesado the cantidadProcesado to set
	 */
	public void setCantidadProcesado(long cantidadProcesado) {
		this.cantidadProcesado = cantidadProcesado;
	}

	/**
	 * @return the cantidadError
	 */
	public long getCantidadError() {
		return cantidadError;
	}

	/**
	 * @param cantidadError the cantidadError to set
	 */
	public void setCantidadError(long cantidadError) {
		this.cantidadError = cantidadError;
	}
}
