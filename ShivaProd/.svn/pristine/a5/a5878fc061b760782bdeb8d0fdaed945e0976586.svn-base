  				<ref bean="aprobarCobroSegunReferenteConMedioDePagoEnProceso"/>
  				<ref bean="rechazarPorTiempoCobroConMedioDePagoEnProceso"/>
  				<ref bean="editarCobroConMedioDePagoEnProceso"/>
  				<ref bean="solicitarImputacionCobroConMedioPagoDefinitivo"/>
  				<ref bean="anularCobroConMedioDePagoEnProceso"/>
  				<ref bean="solicitarEnviarDesapropiacion"/>
  				<!-- Sprint 7 u 8: Fin -->

  				<!-- Sprint 8: Inicio -->
  				<ref bean="registrarCobroMasivoPendienteCobrado"/>
  				<ref bean="registrarCobroMasivoInicioCobrado"/>
  				<!-- Sprint 8: Fin -->
  				
  				<!-- Sprint reingenieria: Inicio -->
<!--   				<ref bean="registrarCobroDePendienteMicAEnProceso"/> -->
  				<ref bean="registrarCobroDeEnProcesoAPendienteMic"/>
  				<ref bean="registrarCobroDePendienteMicAPendienteProcesarMic"/>
  				<ref bean="registrarCobroPendienteProcesarMicAEnProceso"/>
  				<!-- Sprint reingenieria: Fin -->

				<!-- Compensaciones: Sprint 8: Inicio -->
				<ref bean="solicitarImputacionCobroConPedidoAutorizacionSupOperacionesEspeciales"/>
				<ref bean="rechazarAutorizacionSolicitadaAlSupOperacionesEspeciales"/> 
				<ref bean="aprobarCobroSegunSupervisorOperacionesEspecialesSolicitandoImputacion"/> 
				<ref bean="solicitarAutorizacionCobroRechazadoAlSupervisor"/>  
				<!-- Compensaciones: Sprint 8: Fin -->
				
				<!-- Fusion: paquete 1: Inicio -->
				<ref bean="registrarCobroEnProcesoAPendienteDeConfirmacionManual"/>
				<ref bean="registrarCobroPendienteDeConfirmacionManualAEnProceso"/>
				<!-- Fusion: paquete 1: Fin -->

				<!--  NeedIT72562 -->
				<ref bean="registrarCobroEnProcesoDeAplicacionExterna"/>
				<ref bean="registrarCobroCobradoParcialmente"/>
				<ref bean="registrarCobroEnErrorConfirmacionParcial"/>
				
				<ref bean="registrarCobroConfirmadoParcialmenteConError"/>
				
				<ref bean="registrarCobroDeEnProcesoDeAplicacionExternaAErrorEnDesapropiacion"/>
				<ref bean="registrarCobroDePendienteConfirmacionManualYEnProcesoDeAplicacionExternaAErrorEnDesapropiacion"/>
				<ref bean="registrarCobroDeErrorEnDesapropiacionAEnProcesoDeAplicacionExterna"/>
				<ref bean="registrarCobroDeEnErrorEnDesapropiacionAEnProceso"/>
				<ref bean="registrarCobroDeErrorEnDesapropiacionAPendienteConfirmacionManualYEnProcesoDeAplicacionExterna"/>
				


				<!-- - En Proceso de Aplicación Externa(2) ->  Para confirmar o desapropiar -> En Proceso(1*) -->
				<ref bean="registrarProcesoDeAplicacionManualAEnProceso"/>
				<!--  - En Proceso de Aplicación Externa(2) ->-> En Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC (7)-->
				<ref bean="registrarProcesoDeAplicacionExternaAEnProcesoAplicacionExternaConRtaPendienteMIC"/>
				<!-- - En Proceso de Aplicación Extena(2) ->  Tarea pendiente de aprobacion y no existen grupos independientes para procesar -> Pendiente de Confirmación Manual -->
				<ref bean="registrarProcesoDeAplicacionExternaAEnPendienteDeConfirmacionManual"/>
				<!-- - En Proceso de Aplicación Externa(2) ->Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9) COB_PEND_CONFIRMACION_MANUAL_PROCESO_APLIC_EXTERNA -->
				<ref bean="registrarProcesoDeAplicacionExternaAEnPendienteDeConfirmacionManualYEnProcesoAplicacionExterna"/>
				<!-- - En Proceso de Aplicación Externa(2) -> Para confirmar o desapropiar		-> En proceso(1*) -->
				<ref bean="registrarProcesoDeAplicacionExternaAEnProceso"/>

				<!-- En Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC -> // -> En Proceso de Aplicacion Externa y Pendiente de Procesar MIC -->
				<ref bean="registrarProcesoDeAplicacionExternaPendienteMICAEnProcesoDeAplicacionExternaPendienteProcesarMIC"/>
				<!--  - En Proceso de Aplicacion Externa y Pendiente de Procesar MIC (11) -> // -> En Proceso de Aplicación Externa(2)-->
				<ref bean="registrarProcesoDeAplicacionExternaPendienteProcesarMICAEnProcesoDeAplicacionExterna"/>
				
				
				<!--  - Pendiente de Confirmación Manual		->  No existen tareas pendientes de aprobacion -> En Proceso de Aplicación Externa(2)-->
				<ref bean="registrarPendienteConfirmacionManualAEnProcesoDeAplicacionExterna"/>
				
				<!--  - Pendiente de Confirmación Manual		->  Existen tareas pendientes de aprobacion -> Pendiente de Confirmación Manual-->
				<ref bean="registrarPendienteConfirmacionManualAPendienteConfirmacionManual"/>
				
				<!-- - Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9) -> ->  - Pendiente de Confirmación Manual-->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaAEnPendienteConfirmacionManual"/>
				
				<!-- - Pendiente de Confirmación Manual -> ->  - Pendiente de Confirmación Manual  y en Proceso de Aplicación Externa(9)-->
				<ref bean="registrarPendienteConfirmacionManualAEnPendienteConfirmacionManualEnProcesoApliExterna"/>
				
				<!--  - Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9) ->  		// -> Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC-->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaAEnPendienteConfirmacionManualEnProcesoApliExternaPendienteMIC"/>
				<!-- - Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9) ->En Proceso de Aplicación Externa(2) -->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaAEnProcesoDeAplicacionExterna"/>
				<!-- - Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9) -> Pendiente de Confirmación Manual y en Proceso de Aplicación Externa(9)-->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaAPendienteConfirmacionManualEnProcesoDeAplicacionExterna"/>
				
				<!-- Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC  ->
						-> En Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC -->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaPendienteMICAEnProcesoDeAplicacionExternaPendienteMIC"/>
						
				<!-- Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC  ->
						-> Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC -->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaPendienteMICAPendienteConfirmacionManualEnProcesoDeAplicacionExternaPendienteMIC"/>
				
				<!-- Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y con Respuesta Pendiente de MIC  -> 
					Pendiente de Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente de Procesar MIC -->
				<ref bean="registrarPendienteConfirmacionManualEnProcesoApliExternaPendienteMICAEnPendienteConfirmacionManualEnProcesoApliExternaPendienteProcesarMIC"/>
				
				<!--  - Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente de Procesar MIC -> // ->Pendiente de Confirmación Manual y en Proceso de Aplicación Extern -->
				<ref bean="registrarConfirmacionManualEnProcesoApliExternaYPendienteProcesarMICAEnPendienteConfirmacionManualYEnProcesoApliExterna"/>
				<!--  - Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente  MIC -> // -> En Proceso de Aplicacion Externa y Pendiente de Procesar MIC-->
				<ref bean="registrarConfirmacionManualEnProcesoApliExternaYPendienteMICAEnProcesoAplicExternaYPendienteProcesarMIC"/>
				<!--  - Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente de Procesar MIC -> // -> En Proceso de Aplicacion Externa y Pendiente de Procesar MIC-->
				<ref bean="registrarConfirmacionManualEnProcesoApliExternaYPendienteProcesarMICAEnProcesoApliExternaYPendienteProcesarMIC"/>
				<!--  - Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente de Procesar MIC -> // -> Confirmacion Manual, en Proceso de Aplicacion Externa y Pendiente de Procesar MIC-->
				<ref bean="registrarConfirmacionManualEnProcesoApliExternaYPendienteProcesarMICAConfirmacionManualEnProcesoApliExternaYPendienteProcesarMIC"/>
				
				<!--  En Proceso de Aplicacion Externa y Pendiente de Procesar MIC -> // -> En Proceso de Aplicación Externa-->
				<ref bean="registrarProcesoAplicacionExternaYPendienteProcesarMICAEnProcesoAplicacionExterna"/>
				<!--  - Error en Confirmación Parcial ->  Algún grupo del cobro quedó en error de confirmación manual -> Confirmado Parcialmente con error-->
				<ref bean="registrarErrorConfirmacionParcialAEnConfrimadoParcialmenteConError"/>
				<!--  - Error en Desapropiacion Parcial -> // -> En Proceso de Aplicacion Externa-->
<!-- 				<ref bean="registrarErrorDesapropiacionParcialAENProcesoAplicacionExterna"/> -->
				<!--  Error en Desapropiacion -> Bloque no es TA (MIC/SHIVA) -> En Proceso de Aplicacion Externa-->
				<ref bean="registrarErrorDesapropiacionAEnProcesoAplicacionExterna"/>
				<!--  Error en Desapropiacion -> // En proceso-->
				<ref bean="registrarErrorDesapropiacionAEnCobroProceso"/>	
				<!--  -->
				<ref bean="registrarEnProcesoACobroPendienteDesapropiacionManualExterna"/>
				<ref bean="registrarCobroPendienteDesapropiacionManualExternaEnProceso"/>
				
				<ref bean="registrarCobroEnProcesoACobroParcialmenteEnError"/>
				<ref bean="registrarCobroParcialmenteEnErrorACobroParcialmenteImputado"/>
				<ref bean="registrarCobroParcialmenteEnErrorACobroParcialmenteReeditado"/>
				<ref bean="registrarCobroParcialmenteReeditadoACobroReeditado"/>
				
				<ref bean="reeditarParcialmenteCobroErrorEnCobro"/>
				
				<!--  NeedIT72562 FIN -->
  			</list>
  		</property>	
  	</bean>
  	
