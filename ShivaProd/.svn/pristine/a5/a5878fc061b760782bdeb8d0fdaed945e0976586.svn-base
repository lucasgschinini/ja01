  				<ref bean="solicitarAutorizacionReferente"/>  				
  				<ref bean="rechazarAutorizacionSolicitadaAlReferente"/>  				
  				<ref bean="rechazarAutorizacionSolicitadaAlSupervisor"/>
  				<ref bean="anularCobroRechazado"/>
  				<ref bean="aprobarCobroSegunSupervisorSolicitandoImputacion"/>
  				<ref bean="aprobarCobroSegunReferenteSolicitandoImputacion"/>
  				<ref bean="anularCobroEnEdicion"/>
  				<ref bean="registrarErrorDesapropiacion"/>
  				<ref bean="registrarErrorDesapropiacionAErrorEnDesapropiacion"/>
  				<ref bean="reeditarCobroErrorEnApropiacion"/>
  				<ref bean="reeditarCobroErrorEnCobro"/>
  				<ref bean="reeditarCobroErrorConfirmacionParcial"/>
  				<ref bean="anularCobroErrorEnApropiacion"/>
  				<ref bean="anularCobroErrorEnCobro"/>
  				<ref bean="solicitarImputacionCobroConPedidoAutorizacionReferente"/>  				
  				<ref bean="solicitarAutorizacionCobroRechazadoAlReferente"/>  				
  				<!-- Sprint 5: Fin -->

  				<!-- Sprint 6: Inicio -->
  				<ref bean="solicitarImputacionCobroRechazadoConPedidoAutorizacion"/>
  				<ref bean="solicitarVerificacionDebitosCobroRechazado"/>
  				<ref bean="solicitarEdicionCobroRechazado"/>
  				<ref bean="solicitarImputacionCobroRechazado"/>
  				<!-- Sprint 6: Fin -->

