package ar.com.telecom.shiva.negocio.servicios.bean;

import java.io.Serializable;

/**
 * @author u578936 M.A.Uehara
 *
 */
public class ErrorBean implements Serializable {
	private static final long serialVersionUID = 20180731L;

	private Integer prioridad = -1;
	private String error = "";
	private String descripcionError = "";


	public ErrorBean() {}


	/**
	 * @return the prioridad
	 */
	public Integer getPrioridad() {
		return prioridad;
	}


	/**
	 * @param prioridad the prioridad to set
	 */
	public void setPrioridad(Integer prioridad) {
		this.prioridad = prioridad;
	}

	/**
	 * @return the error
	 */
	public String getError() {
		return error;
	}

	/**
	 * @param error the error to set
	 */
	public void setError(String error) {
		this.error = error;
	}
	/**
	 * @return the descripcionError
	 */
	public String getDescripcionError() {
		return descripcionError;
	}
	/**
	 * @param descripcionError the descripcionError to set
	 */
	public void setDescripcionError(String descripcionError) {
		this.descripcionError = descripcionError;
	}
	
	
	public static final Integer PRIORIDAD_OBLIGATORIEDAD = 0;
	public static final Integer PRIORIDAD_ERROR_FORMATO = 2;
	public static final Integer PRIORIDAD_ERROR_LONGITUD = 1;
	public static final Integer PRIORIDAD_NO_VACIO = 0;
	public static final Integer PRIORIDAD_DOMINIO = 3;
	
	public static final Integer PRIORIDAD_EXCLUIDO = 3;

	public static final Integer PRIORIDAD_OPCION = 4;
	
	
	
	
}
