package ar.com.telecom.shiva.negocio.dto.cobros.simulacion;

import java.math.BigDecimal;

import ar.com.telecom.shiva.base.enumeradores.EstadoAcuerdoFacturacionEnum;
import ar.com.telecom.shiva.base.enumeradores.EstadoCargoGeneradoEnum;
import ar.com.telecom.shiva.base.enumeradores.SistemaEnum;


public class ResultadoSimulacionDescobroFactura extends ResultadoSimulacionDescobro {
	
	private SistemaEnum sistemaCcontracargo;
	private Long idClienteLegadoContracargo;
	private Long acuerdoContracargo;
	private EstadoAcuerdoFacturacionEnum estadoAcuerdoContracargo;
	private EstadoCargoGeneradoEnum estadoCargoGenerado;	
	private BigDecimal cobradorInteresesTrasladadosRegulados;
	private BigDecimal cobradorInteresesTrasladadosNoRegulados;
	private boolean requiereBuscarAcuerdoActivo = false;
	

	public EstadoCargoGeneradoEnum getEstadoCargoGenerado() {
		return estadoCargoGenerado;
	}

	public void setEstadoCargoGenerado(EstadoCargoGeneradoEnum estadoCargoGenerado) {
		this.estadoCargoGenerado = estadoCargoGenerado;
	}

	public EstadoAcuerdoFacturacionEnum getEstadoAcuerdoContracargo() {
		return estadoAcuerdoContracargo;
	}

	public void setEstadoAcuerdoContracargo(
			EstadoAcuerdoFacturacionEnum estadoAcuerdoContracargo) {
		this.estadoAcuerdoContracargo = estadoAcuerdoContracargo;
	}

	public Long getIdClienteLegadoContracargo() {
		return idClienteLegadoContracargo;
	}

	public void setIdClienteLegadoContracargo(Long idClienteLegadoContracargo) {
		this.idClienteLegadoContracargo = idClienteLegadoContracargo;
	}

	public Long getAcuerdoContracargo() {
		return acuerdoContracargo;
	}

	public void setAcuerdoContracargo(Long acuerdoContracargo) {
		this.acuerdoContracargo = acuerdoContracargo;
	}

	public SistemaEnum getSistemaCcontracargo() {
		return sistemaCcontracargo;
	}

	public void setSistemaCcontracargo(SistemaEnum sistemaCcontracargo) {
		this.sistemaCcontracargo = sistemaCcontracargo;
	}

	public BigDecimal getCobradorInteresesTrasladadosRegulados() {
		return cobradorInteresesTrasladadosRegulados;
	}

	public void setCobradorInteresesTrasladadosRegulados(
			BigDecimal cobradorInteresesTrasladadosRegulados) {
		this.cobradorInteresesTrasladadosRegulados = cobradorInteresesTrasladadosRegulados;
	}

	public BigDecimal getCobradorInteresesTrasladadosNoRegulados() {
		return cobradorInteresesTrasladadosNoRegulados;
	}

	public void setCobradorInteresesTrasladadosNoRegulados(
			BigDecimal cobradorInteresesTrasladadosNoRegulados) {
		this.cobradorInteresesTrasladadosNoRegulados = cobradorInteresesTrasladadosNoRegulados;
	}
	public boolean isRequiereBuscarAcuerdoActivo() {
		return requiereBuscarAcuerdoActivo;
	}
	public void setRequiereBuscarAcuerdoActivo(boolean requiereBuscarAcuerdoActivo) {
		this.requiereBuscarAcuerdoActivo = requiereBuscarAcuerdoActivo;
	}
	
}
