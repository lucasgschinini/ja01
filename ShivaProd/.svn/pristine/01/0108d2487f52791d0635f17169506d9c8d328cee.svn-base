<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cm="http://www.telecom.com.ar/XMLSchema/bios/common" xmlns:rqm="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage">
	<xs:import namespace="http://www.telecom.com.ar/XMLSchema/bios/common" schemaLocation="Diccionario.xsd"/>
	<xs:element name="inactivarClienteEnLegadosReq">
		<xs:complexType>
			<xs:choice>
				<xs:element name="legados">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="legado" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="legadoID" use="required">
										<xs:annotation>
											<xs:documentation>ID del Sistema Legado</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:whiteSpace value="collapse"/>
												<xs:minLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="legadoOfID" use="required">
										<xs:annotation>
											<xs:documentation>Código de Of. Comercial (ebos) de Legado</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:whiteSpace value="collapse"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="legadoClienteID" use="required">
										<xs:annotation>
											<xs:documentation>ID del Cliente en el Legado</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:whiteSpace value="collapse"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="crm">
					<xs:complexType>
						<xs:attribute name="legadoID" use="required">
							<xs:annotation>
								<xs:documentation>ID del Sistema Legado</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="legadoOfID" use="required">
							<xs:annotation>
								<xs:documentation>Código de Of. Comercial (ebos) de Legado</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:attribute name="crmClienteID" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>