				+ separador + " viaPago=" + viaPago + separador + " bloqueoPago=" + bloqueoPago
				+ separador + " descripcionBloqueo=" + descripcionBloqueo
				+ separador + " numeroDocSAPVinculado=" + numeroDocSAPVinculado
				+ separador + " ejercicioFiscalDocSAPVinculado="
				+ ejercicioFiscalDocSAPVinculado + separador + " posicionDocSAPVinculado="
