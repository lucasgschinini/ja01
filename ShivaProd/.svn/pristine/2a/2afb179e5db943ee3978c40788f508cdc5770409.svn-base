  		<property name="evento" value="iniciarDescobroOrigenCobrador"/>
  		<property name="origen" ref="des_imputar"/>
  		<property name="destino" ref="des_descobro_proceso"/>	
  	</bean>		
  	
  	<bean id="regPendReversoDocMedioCobradores" class="ar.com.telecom.shiva.negocio.workflow.definicion.Transicion">
