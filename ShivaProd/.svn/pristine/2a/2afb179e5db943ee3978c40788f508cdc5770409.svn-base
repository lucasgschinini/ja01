  			<util:constant 
  			static-field="ar.com.telecom.shiva.negocio.workflow.definicion.TipoWorkflow.DESCOBRO_ORIGEN_COBRADOR" />
 		</property>
  		
  		<property name="transiciones">
  			<list>
  				<ref bean="inicioDES" />
  				<ref bean="iniciarDescobroOrigenCobrador" />
   				<ref bean="regPendReversoDocMedioCobradores" /> 
  				<ref bean="regPendReversoDocCobradoresMedioCalipso" /> 
  				<ref bean="regPendReversoDocCobradoresMedioMic" /> 
  				<ref bean="regPendReversoDocCalipsoMedioCobradores" /> 
  				<ref bean="regPendReversoDocMicMedioCobradores" /> 
   				<ref bean="regPendReversoDocCobradores" />
  				<ref bean="regPendReversoMedioCobradores" />
  				<ref bean="regPendReversoDocMedioCalipso" />  				
  				<ref bean="regPendReversoDocCalipso" />  				
  				<ref bean="regPendReversoMedioCalipso" />  				
  				<ref bean="regPendReversoDocMedioMic" />  				
  				<ref bean="regPendReversoDocMic" />  				
  				<ref bean="regPendReversoMedioMic" /> 		
    			<ref bean="procPendReversoDocMedioCobradores" /> 
  				<ref bean="procPendReversoDocCobradoresMedioCalipso" /> 
  				<ref bean="procPendReversoDocCobradoresMedioMic" /> 
  				<ref bean="procPendReversoDocCalipsoMedioCobradores" /> 
  				<ref bean="procPendReversoDocMicMedioCobradores" />
    			<ref bean="procPendReversoDocCobradores" />
