  	<util:constant id="des_descobro_proceso"
  			static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.DES_DESCOBRO_PROCESO" />
  	<util:constant id="des_ambos_cobradores"
  			static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.DES_AMBOS_COBRADORES" />
	<util:constant id="des_doc_cobradores_med_calipso"
  			static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.DES_DOCUMENTOS_COBRADORES_MEDIOS_CALIPSO" />  	
	<util:constant id="des_doc_cobradores_med_mic"
  			static-field="ar.com.telecom.shiva.negocio.workflow.definicion.Estado.DES_DOCUMENTOS_COBRADORES_MEDIOS_MIC" />
