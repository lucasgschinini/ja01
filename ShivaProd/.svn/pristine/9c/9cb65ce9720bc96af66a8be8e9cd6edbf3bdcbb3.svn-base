<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>shiva</groupId>
  <artifactId>shiva</artifactId>
  <version>${fecha.version}-${appl.version}</version>
  <packaging>war</packaging>
  
  <properties>
 		<!-- Versiones propias -->
 		<fecha.version>20160622</fecha.version>
 		<nro.version>2.05.00</nro.version>
 		<num.entrega>.22</num.entrega>
 		
 		<num.version>v${nro.version}</num.version>
 		<appl.version>${num.version}${num.entrega}</appl.version>
 		<entrega.version>Version ${nro.version}</entrega.version>
 		
 		<!-- Versiones de Librerias -->
		<spring-version>3.2.6.RELEASE</spring-version>
		<spring-security-version>3.2.0.RELEASE</spring-security-version>
		<slf4j-version>1.7.5</slf4j-version>
		<logback-version>1.0.13</logback-version>
		<drools-version>5.5.0.Final</drools-version>
		<jbpm-version>5.4.0.Final</jbpm-version>
		<websphere-mq-version>7.5.0.0</websphere-mq-version>
		<metro-version>2.2.8</metro-version>
		<cxf-version>2.7.6</cxf-version>
	</properties>
  
  	<build>
		<directory>target</directory>
		<sourceDirectory>src/main/java</sourceDirectory>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
		</resources>
		<outputDirectory>target/classes</outputDirectory>
		
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.7</version>
				<executions>
					<execution>
						<id>1: Preparar fuentes</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>src/main/java</source>
								<source>src/main/resources</source>
							</sources>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<!-- CXF -->
			<plugin>
				<groupId>org.apache.cxf</groupId>
				<artifactId>cxf-codegen-plugin</artifactId>
				<version>${cxf-version}</version>
				<executions>
					<execution>
						<id>2: Generar fuentes para WS</id>
						<phase>generate-sources</phase>
						<configuration>
							<defaultOptions>
							   	<bindingFiles>
									<bindingFile>${basedir}/src/main/resources/wsdl/binding.xml</bindingFile>
								</bindingFiles>
								<noAddressBinding>true</noAddressBinding>
							</defaultOptions>
							<sourceRoot>${basedir}/src/main/java</sourceRoot>
							<wsdlOptions>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/siebel/ClienteConsultarContract.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.siebel</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/delta/WebServiceTeamComercialCliente.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.delta</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoCobranzas.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.cobranzas</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoConsultaAcuerdo.wsdl</wsdl>
									<extraargs>
										<extraarg>-autoNameResolution</extraarg>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.consultaAcuerdo</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoCargos.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.cargos</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceCalipsoConsultaDeudayCredito.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.consultadeudaycredito</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/deimos/WebServiceDeimos.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.deimos</extraarg>
									</extraargs>
								</wsdlOption>
								<wsdlOption>
									<wsdl>${basedir}/src/main/resources/wsdl/calipso/WebServiceReversionCobros.wsdl</wsdl>
									<extraargs>
										<extraarg>-p</extraarg>
										<extraarg>ar.com.telecom.shiva.presentacion.wsdl.calipso.descobros</extraarg>
									</extraargs>
								</wsdlOption>
							</wsdlOptions>
						</configuration>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.0</version>
				<executions>
					<execution>
						<id>3: Compilacion</id>
						<phase>compile</phase>
					</execution>
				</executions>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<compilerArgument>-XDignore.symbol.file</compilerArgument>
				</configuration>
			</plugin>
			<!-- 
			Permite ejecutar las pruebas unitarias (Test)
			En este caso (por ser skipTests=true) no voy a ejecutar ninguna prueba 
			y se saltea 
			-->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.12.4</version>
				<executions>
					<execution>
	               		<id>4: Pruebas unitarias</id>
	                  	<phase>test</phase>
	                </execution> 
				</executions>
				<configuration>
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>5: Preparar Jar para el Batch</id>
	                  	<phase>process-classes</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  	<tasks>
	                 		<echo>Preparando el JAR para el Batch</echo>
	                 		
	                 		<delete file="src/main/resources/logback.xml" />
	                 		<delete file="${project.build.outputDirectory}/logback.xml" />
	                     	<copy file="src/main/resources/backup/logback/logback-batch.xml" 
	                     	   		tofile="src/main/resources/logback.xml"/>
	                     	<copy file="src/main/resources/backup/logback/logback-batch.xml" 
	                     	   		tofile="${project.build.outputDirectory}/logback.xml"/>
	                  </tasks>
	                  </configuration>
	               </execution>
	            </executions>
	        </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.4</version>
				<executions>
					<execution>
						<id>6: Crear JAR para el Batch</id>
						<phase>process-classes</phase>
						<goals>
							<goal>jar</goal>
						</goals>
						<configuration>
							<finalName>${project.artifactId}-${project.version}-Batch</finalName>
							<excludes>
		                		<exclude>**/spring-context.xml</exclude>
		                		<exclude>**/spring-jdbc-x-war*.xml</exclude>
		                		<exclude>**/spring-security-x-war*.xml</exclude>
		                		<exclude>**/spring-captcha*.xml</exclude>
		                		<exclude>**/shiva*.bat</exclude>
		                		<exclude>**/shiva*.sh</exclude>
		                		<exclude>**/logback-*.xml</exclude>
		              		</excludes>
						</configuration>
					</execution>
        		</executions>	
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>9: Preparar WAR</id>
	                  	<phase>prepare-package</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  		<tasks>
	                  			<echo>Preparando la carpeta de shiva_batch</echo>
	                  			<delete dir="${project.build.directory}/entrega/${appl.version}"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/fuentes"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/war"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/sql"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/configuracion"/>
	                  			
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/retenciones"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/subdiario"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/morosidad"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/conciliacion"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/operacionesTruncas"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/scard"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/tagetik"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/reversion"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/contabilidad"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/migracion"/>
	                 			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/team"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/shiva"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/shiva/lib"/>
	                  			<mkdir dir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/shiva/tmp"/>
	                  			
	                  			<echo>Copiando los scripts sh...</echo>
	                  			<copy todir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch">
    								<fileset dir="src/main/resources/backup/batch">
        								<include name="**/*.sh"/>  
    								</fileset>
								</copy>
	                  			
	                  			<echo>Moviendo el jar Shiva-Batch...</echo>
	                  			<move todir="${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/shiva">
    								<fileset dir="${project.build.directory}">
        								<include name="**/${project.artifactId}-${project.version}*.jar"/>  
    								</fileset>
								</move>
	                  			
	                  			<echo>Copiando los scripts SQL...</echo>
	                  			<copy todir="${project.build.directory}/entrega/${appl.version}/sql">
    								<fileset dir="${project.basedir}/enCurso/versiones/${num.version}">
        								<include name="**/*.*"/>
    								</fileset>
								</copy>
								
								<echo>Copiando los archivos de configuracion...</echo>
								<copy todir="${project.build.directory}/entrega/${appl.version}/configuracion">
    								<fileset dir="src/main/resources">
        								<include name="**/shiva_desa.properties"/>
        								<include name="**/shiva_desa2.properties"/>
        								<include name="**/shiva_desa3.properties"/>  
        								<include name="**/shiva_inte.properties"/>
        								<include name="**/shiva_pau.properties"/>
        								<include name="**/shiva_prod.properties"/>
    								</fileset>
								</copy>
								
								<echo>Preparando el WAR...</echo>
	                 			<delete file="src/main/resources/logback.xml" />
	                 			<delete file="${project.build.outputDirectory}/logback.xml" />
	                 			
	                 			<copy file="src/main/resources/backup/logback/logback-jboss.xml" 
	                     	   		tofile="src/main/resources/logback.xml"/>
	                     	   	<copy file="src/main/resources/backup/logback/logback-jboss.xml" 
	                     	   		tofile="${project.build.outputDirectory}/logback.xml"/>
	                  		</tasks>
	                  	</configuration>
	               </execution>
	            </executions>
	        </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>2.5.1</version>
				<executions>
					<execution>
						<id>11.1: Copiar dependencias</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/entrega/${appl.version}/batch/shiva_batch/shiva/lib</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>false</overWriteSnapshots>
							<overWriteIfNewer>true</overWriteIfNewer>
						</configuration>
				  	</execution>
				 </executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.3</version>
				<executions>
					<execution>
						<id>12: Crear WAR</id>
						<phase>package</phase>
					</execution>
				</executions>
				<configuration>
					<outputDirectory>${project.build.directory}/entrega/${appl.version}/war</outputDirectory>
					<warSourceDirectory>WebContent</warSourceDirectory>
					<failOnMissingWebXml>false</failOnMissingWebXml>
					<packagingExcludes>
						**/spring-context-batch.xml,
						**/spring-jdbc-x-jar*.xml,
						**/spring-security-x-jar*.xml,
						**/spring-context-batch*.xml,
						**/logback-*.xml,
						**/shiva*.bat,
						**/shiva*.sh
					</packagingExcludes>
				</configuration>
			</plugin>
			<plugin>
            	<groupId>org.apache.maven.plugins</groupId>
            	<artifactId>maven-antrun-plugin</artifactId>
            	<version>1.1</version>
	            <executions>
	               	<execution>
	               		<id>13: Ultimos</id>
	                  	<phase>install</phase>
	                  	<goals>
	                     	<goal>run</goal>
	                  	</goals>
	                  	<configuration>
	                  		<tasks>
	                  			<echo>Zipeando Shiva-Batch...</echo>
	                  			<zip destfile="${project.build.directory}/entrega/${appl.version}/batch/${project.artifactId}-${project.version}.batch.zip"
	                  				 basedir="${project.build.directory}/entrega/${appl.version}/batch" />
	                  			
	                  			<echo>Zipeando Sources...</echo>
								<zip destfile="${project.build.directory}/entrega/${appl.version}/fuentes/${project.artifactId}-${project.version}-sources.zip">
									<zipfileset dir="${project.basedir}/src" prefix="src" />
									<zipfileset dir="${project.basedir}/documentos" prefix="documentos" />
									<zipfileset dir="${project.basedir}/WebContent" prefix="WebContent" excludes="WEB-INF/lib/**"/>
									<zipfileset dir="." includes="pom.xml,.project,.classpath"/>
								</zip>
	                  				 
	                  			<echo>Preparando versión en la carpeta de entregas...</echo>
	                  			<delete dir="${project.basedir}/entregas/${entrega.version}"/>
	                  			<mkdir dir="${project.basedir}/entregas/${entrega.version}"/>
	                  			<mkdir dir="${project.basedir}/entregas/${entrega.version}/fuentes"/>
	                  			<mkdir dir="${project.basedir}/entregas/${entrega.version}/sql"/>
	                  			
	                  			<copy todir="${project.basedir}/entregas/${entrega.version}/sql">
    								<fileset dir="${project.basedir}/enCurso/versiones/${num.version}">
        								<include name="**/*.*"/>  
    								</fileset>
								</copy>
								
								<copy todir="${project.basedir}/entregas/${entrega.version}/fuentes">
    								<fileset dir="${project.build.directory}/entrega/${appl.version}/fuentes">
        								<include name="**/*sources.zip"/>  
    								</fileset>
								</copy>
	                  		</tasks>
	                  	</configuration>
	               </execution>
	            </executions>
	        </plugin>
		</plugins>
	</build>
  
	<repositories>
		<repository>
			<id>jboss</id>
			<url>http://repository.jboss.org/nexus/content/groups/public/</url>
		</repository>
		<repository>
			<id>redhat-techpreview-all-repository</id>
			<url>http://maven.repository.redhat.com/techpreview/all/</url>
		</repository>
	</repositories>
 
