<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        
	<bean id="motorProcesoConciliacion" class="ar.com.telecom.shiva.negocio.conciliacion.MotorProcesoConciliacion">
  		<property name="reglas">
  			<list>
  				<ref bean="BOLETA-DEPOSITO-CHEQUE" />
  				<ref bean="BOLETA-DEPOSITO-CHEQUE-PD" />
  				<ref bean="BOLETA-DEPOSITO-EFECTIVO" />
				<ref bean="BOLETA-DEPOSITO-SIN-VALOR" /> 
  			</list>
  		</property>
  	</bean>
  	
<!--   	<import resource="reglaPruebas.xml" /> -->
  	<import resource="reglasConciliacionBoletaDepositoCheque.xml" />
  	<import resource="reglasConciliacionBoletaDepositoChequePagoDiferido.xml" />
  	<import resource="reglasConciliacionBoletaDepositoEfectivo.xml" />
  	<import resource="reglasConciliacionBoletaDepositoSinValor.xml" />
  	
  	
   	<!--   	estos bean los reutilizo para todos los casos -->
  	<bean id="tipoValor" class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="Tipo Valor"/>
  		<property name="atributoBoleta" value="tipoValor"/>
  		<property name="atributoRegistro" value="tipoValor"/>
  	</bean>
  	
  	<bean id="nroCliente"  class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="Nro Cliente"/>
  		<property name="atributoBoleta" value="codCliente"/>
  		<property name="atributoRegistro" value="codigoCliente"/>
  	</bean>
  	<bean id="nroCheque"  class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="Nro Cheque"/>
  		<property name="atributoBoleta" value="numeroCheque"/>
  		<property name="atributoRegistro" value="numeroCheque"/>
  	</bean>
  	<bean id="importe"  class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="Importe"/>
  		<property name="atributoBoleta" value="importeParaComparar"/>
  		<property name="atributoRegistro" value="importeParaComparar"/>
  	</bean>
  	<bean id="nroBoleta"  class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="Nro Boleta"/>
  		<property name="atributoBoleta" value="numeroBoleta"/>
  		<property name="atributoRegistro" value="nroBoleta"/>
  	</bean>
  	<bean id="acuerdo"  class="ar.com.telecom.shiva.negocio.conciliacion.definicion.CampoCoincidente">
  		<property name="nombre" value="acuerdo"/>
  		<property name="atributoBoleta" value="acuerdo"/>
  		<property name="atributoRegistro" value="idAcuerdo"/>
  	</bean>
  	
</beans>