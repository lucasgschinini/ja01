package ar.com.telecom.shiva.persistencia.bean;

import ar.com.telecom.shiva.base.enumeradores.TipoCobroHiloEnum;

public class ResultadoBusquedaCantidadHilosEnCurso {
	
	private Integer cantidadHilosEnCurso;
	private TipoCobroHiloEnum tipoHilo;
	
	public ResultadoBusquedaCantidadHilosEnCurso(){
		
		this.cantidadHilosEnCurso = 0;
		this.tipoHilo = null;
	}
	
	public ResultadoBusquedaCantidadHilosEnCurso(Integer cantidadHilosEnCurso, TipoCobroHiloEnum tipoHilo) {
		
		this.cantidadHilosEnCurso = cantidadHilosEnCurso;
		this.tipoHilo = tipoHilo;
	}
	public Integer getCantidadHilosEnCurso() {
		return cantidadHilosEnCurso;
	}
	public void setCantidadHilosEnCurso(Integer cantidadHilosEnCurso) {
		this.cantidadHilosEnCurso = cantidadHilosEnCurso;
	}
	public TipoCobroHiloEnum getTipoHilo() {
		return tipoHilo;
	}
	public void setTipoHilo(TipoCobroHiloEnum tipoHilo) {
		this.tipoHilo = tipoHilo;
	}
	
	

}
