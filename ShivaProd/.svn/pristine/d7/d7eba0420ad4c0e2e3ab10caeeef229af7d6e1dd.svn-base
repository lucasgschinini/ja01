package ar.com.telecom.shiva.negocio.dto;

import java.math.BigDecimal;
import java.util.Date;

public class ValorPorReversionDto extends RegistroAVCDto {

	private static final long serialVersionUID = 1L;

	private String idTipoValor;
	private BigDecimal saldoDisponible;
	private String numeroCheque;
	private String idBancoOrigen;//En caso de ser un interdeposito en este campo se guarda el id del banco Deposito
	private String fechaDeposito;
	private Date fechaVencimiento;
	private String numeroBoleta;
	private String numeroReferencia;
	private String codigoOrganismo;
	private String codigoInterdeposito;
	
	public String getIdTipoValor() {
		return idTipoValor;
	}
	public void setIdTipoValor(String idTipoValor) {
		this.idTipoValor = idTipoValor;
	}
	public BigDecimal getSaldoDisponible() {
		return saldoDisponible;
	}
	public void setSaldoDisponible(BigDecimal saldoDisponible) {
		this.saldoDisponible = saldoDisponible;
	}
	public String getNumeroCheque() {
		return numeroCheque;
	}
	public void setNumeroCheque(String numeroCheque) {
		this.numeroCheque = numeroCheque;
	}
	public String getIdBancoOrigen() {
		return idBancoOrigen;
	}
	public void setIdBancoOrigen(String idBancoOrigen) {
		this.idBancoOrigen = idBancoOrigen;
	}
	public String getFechaDeposito() {
		return fechaDeposito;
	}
	public void setFechaDeposito(String fechaDeposito) {
		this.fechaDeposito = fechaDeposito;
	}
	public Date getFechaVencimiento() {
		return fechaVencimiento;
	}
	public void setFechaVencimiento(Date fechaVencimiento) {
		this.fechaVencimiento = fechaVencimiento;
	}
	public String getNumeroReferencia() {
		return numeroReferencia;
	}
	public void setNumeroReferencia(String numeroReferencia) {
		this.numeroReferencia = numeroReferencia;
	}
	public String getCodigoOrganismo() {
		return codigoOrganismo;
	}
	public void setCodigoOrganismo(String codigoOrganismo) {
		this.codigoOrganismo = codigoOrganismo;
	}
	public String getCodigoInterdeposito() {
		return codigoInterdeposito;
	}
	public void setCodigoInterdeposito(String codigoInterdeposito) {
		this.codigoInterdeposito = codigoInterdeposito;
	}
	public String getNumeroBoleta() {
		return numeroBoleta;
	}
	
	public void setNumeroBoleta(String numeroBoleta) {
		this.numeroBoleta = numeroBoleta;
	}
	
}
