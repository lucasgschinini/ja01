package ar.com.telecom.shiva.test.seguridad;

import ar.com.telecom.shiva.base.utils.Encriptador;

public class EncriptacionPasswords {
	
	public static void main(String[] args) throws Exception {
		String claveOriginal = "Kk69hFHn";
		String claveEncriptada = Encriptador.encrypt(claveOriginal);
		
		System.out.println("Clave Original: " + claveOriginal);
		System.out.println("Clave Encriptada:" + claveEncriptada);
		
	}
}
	
