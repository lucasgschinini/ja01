<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE taglib 
	PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
       	   "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>menuGenerico</short-name>
	<description>Genereador de Menu</description>
	<tag>
		<name>generadorMenu</name>
		<tag-class>ar.com.telecom.shiva.presentacion.bean.taglib.GeneradorMenu</tag-class>
		<body-content>JSP</body-content>
	
		<attribute>
			<name>titulo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Titulo del menu</description>
		</attribute>
	
	</tag>
	
</taglib>
