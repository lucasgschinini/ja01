<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.telecom.com.ar/XMLSchema/bios/common" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cm="http://www.telecom.com.ar/XMLSchema/bios/common">
	<!-- **********************************************************-->
	<!-- *********** Datos genericos  *************************-->
	<!-- **********************************************************-->
	<xs:element name="nombre">
		<xs:annotation>
			<xs:documentation>Nombre</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="apellido" type="xs:string">
		<xs:annotation>
			<xs:documentation>Apellido</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="scoring" type="xs:string"/>	
	<xs:element name="perfilCRM" type="xs:string"/>
	<xs:element name="pin" type="xs:string"/>
							
	<xs:element name="tratamiento" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tratamiento al Nombre</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="telefono" type="xs:string">
		<xs:annotation>
			<xs:documentation>Telefono</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="condicionIVA" type="xs:string">
		<xs:annotation>
			<xs:documentation>Condicion de IVA</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="cuit" type="xs:string">
		<xs:annotation>
			<xs:documentation>Numero de CUIT</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ufUsuario" type="xs:string"/>
	<xs:element name="usuario">
		<xs:annotation>
			<xs:documentation>Identificador de Usuario</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="tipo">
						<xs:annotation>
							<xs:documentation>Tipo de usuario</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="servicio">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="cm:codigo">
					<xs:annotation>
						<xs:documentation>codigo de servicio</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="cm:identificador">
					<xs:annotation>
						<xs:documentation>Identificador univoco de servicio</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="modelo" type="xs:string"/>
	<xs:element name="tipo" type="xs:string"/>
	<xs:element name="identificador" type="xs:string">
		<xs:annotation>
			<xs:documentation>Identificador Generico</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="propiedades">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="propiedad" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="nombre" type="xs:string"/>
							<xs:element name="valor" type="xs:string"/>
							<xs:element name="descripcion" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="producto">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="cm:propiedades" minOccurs="0"/>
				<xs:element name="servicios" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="servicio" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="cm:propiedades"/>
										<xs:element ref="cm:serviciosAdicionales" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="detalleServicio">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="cm:identificador">
					<xs:annotation>
						<xs:documentation>Identificador univoco de servicio</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="propiedades" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="propiedad" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="nombre" type="xs:string"/>
										<xs:element name="valor" type="xs:string"/>
										<xs:element name="descripcion"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="prioridad" type="xs:string"/>
	<xs:element name="cantidad" type="xs:integer"/>
	<xs:element name="codigo" type="xs:string"/>
	<xs:element name="sintoma" type="xs:string"/>
	<xs:element name="motivoLlamado" type="xs:string"/>
	<xs:element name="reclamoMarcaRetencion" type="xs:string"/>
	<xs:element name="reclamoTipoRetencion" type="xs:string"/>
	<xs:element name="reclamoMarcaVip" type="xs:string"/>
	<xs:element name="reclamoMarcaRecomendado" type="xs:string"/>
	<xs:element name="reclamoFechaIngresoRed" type="xs:string"/>
	<xs:element name="reclamoOrigenGirafe" type="xs:string"/>
	<xs:element name="reclamoTipoOrigenGirafe" type="xs:string"/>
	<xs:element name="estado" type="xs:string"/>
	<xs:element name="fecha">
		<xs:annotation>
			<xs:documentation>Fecha formato (yyyy-mm-dd)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(19|20)\d\d-(0[1-9]|10|11|12)-(0[1-9]|1\d|2\d|30|31)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="hora">
		<xs:annotation>
			<xs:documentation>Hora (hh24:mi:ss)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(0\d|1\d|2[0-3]):([0-5]\d):([0-5]\d)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="fechaHoraDesde">
		<xs:annotation>
			<xs:documentation>fecha con formato (yyyy-mm-dd hh24:mi:ss)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(19|20)\d\d-(0[1-9]|10|11|12)-(0[1-9]|1\d|2\d|30|31) (0\d|1\d|2[0-3]):([0-5]\d):([0-5]\d)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="descripcion" type="xs:string"/>
	<xs:element name="fechaHora">
		<xs:annotation>
			<xs:documentation>fecha con formato (yyyy-mm-dd hh24:mi:ss)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(19|20)\d\d-(0[1-9]|10|11|12)-(0[1-9]|1\d|2\d|30|31) (0\d|1\d|2[0-3]):([0-5]\d):([0-5]\d)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="fechaHoraHasta">
		<xs:annotation>
			<xs:documentation>fecha con formato (yyyy-mm-dd hh24:mi:ss)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(19|20)\d\d-(0[1-9]|10|11|12)-(0[1-9]|1\d|2\d|30|31) (0\d|1\d|2[0-3]):([0-5]\d):([0-5]\d)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="accion" type="xs:string"/>
	<xs:element name="observaciones" type="xs:string"/>
	<xs:element name="profesion" type="xs:string"/>
	<xs:element name="marcaVIP" type="xs:string"/>
	<xs:element name="resultado" type="xs:string"/>
	<xs:element name="identificadorEntidadProveedor" type="xs:string"/>
	<xs:element name="importe" type="xs:string"/>
	<!-- **********************************************************-->
	<!-- Datos comunes                                     *********** -->
	<!-- **********************************************************-->
	<xs:element name="origen">
		<xs:annotation>
			<xs:documentation>Proceso de negocio que origina la tarea</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="ASEG"/>
				<xs:enumeration value="PROV"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="grupo">
		<xs:annotation>
			<xs:documentation>Grupo operativo que recibe la tarea</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="PI"/>
				<xs:enumeration value="SSEE"/>
				<xs:enumeration value="PE"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="tarea" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tarea</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="categoriaCliente" type="xs:string"/>
	<!-- **********************************************************-->
	<!-- Datos para prueba de linea  (globales) *********** -->
	<!-- **********************************************************-->
	<xs:element name="usuarioPrueba" type="xs:string"/>
	<xs:element name="tipoPrueba">
		<xs:annotation>
			<xs:documentation>Tipo de prueba</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="TST-FULL"/>
				<xs:enumeration value="TST-SOAK"/>
				<xs:enumeration value="TST-MEAS-G"/>
				<xs:enumeration value="TST-MEAS-M"/>
				<xs:enumeration value="TST-PORT"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="historicoPrueba">
		<xs:annotation>
			<xs:documentation>Historico que se desea consultar</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="HST-LINE"/>
				<xs:enumeration value="HST-PORT"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!-- **********************************************************-->
	<!-- Datos para prueba de linea  (Medicion Linea) *** -->
	<!-- **********************************************************-->
	<xs:element name="verCode">
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="diagnosticoUltimaPruebaActor" type="xs:string"/>
	<xs:element name="fechaHoraUltimaPruebaDSL">
		<xs:annotation>
			<xs:documentation>fecha con formato (yyyy-mm-dd hh24:mi:ss)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="(19|20)\d\d-(0[1-9]|10|11|12)-(0[1-9]|1\d|2\d|30|31) (0\d|1\d|2[0-3]):([0-5]\d):([0-5]\d)"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="summaryCodes" type="xs:string"/>
	<xs:element name="sumaryCode1" type="xs:string"/>
	<xs:element name="sumaryCode2" type="xs:string"/>
	<xs:element name="sumaryCode3" type="xs:string"/>
	<xs:element name="sumaryCode4" type="xs:string"/>
	<xs:element name="sumaryCode5" type="xs:string"/>
	<xs:element name="sumaryCode6" type="xs:string"/>
	<xs:element name="ln1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Loop length</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ln2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Open distance from CO or RT</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ln3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Open distance from station</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nse" type="xs:string">
		<xs:annotation>
			<xs:documentation>Noise (in tst-full metalic noise) </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the real portion of tree Terminal AC adminttance TIP to ground in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the imaginary portion of tree Terminal AC adminttance TIP to ground in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the real portion of tree Terminal AC adminttance Ring to ground in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac4" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the imaginary portion of tree Terminal AC adminttance Ring to ground in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac5" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the real portion of tree Terminal AC adminttance TIP to Ring in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ntsac6" type="xs:string">
		<xs:annotation>
			<xs:documentation>Inverse on the imaginary portion of tree Terminal AC adminttance TIP to Ring in Kohms</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzgnse" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ground noise (Aux 16)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrab" type="xs:string">
		<xs:annotation>
			<xs:documentation>Voltage A to B calculated (raw)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrae" type="xs:string">
		<xs:annotation>
			<xs:documentation>Resistance A to earth</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrav" type="xs:string">
		<xs:annotation>
			<xs:documentation>Resistance A to battery</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrba" type="xs:string">
		<xs:annotation>
			<xs:documentation>Resistance B to A</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrbe" type="xs:string">
		<xs:annotation>
			<xs:documentation>Voltage B to earth (raw)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzrbv" type="xs:string">
		<xs:annotation>
			<xs:documentation>Resistance B to battery</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzvab" type="xs:string">
		<xs:annotation>
			<xs:documentation>Voltage A to B calculated (raw)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzvae" type="xs:string">
		<xs:annotation>
			<xs:documentation>Voltage A to earth (raw)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="nzvbe" type="xs:string">
		<xs:annotation>
			<xs:documentation>Voltage B to earth (raw)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to Ring AC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to ground AC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ring to ground AC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac4" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to Ring Capacitance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac5" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to ground Capacitance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ac6" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ring to ground Capacitance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="af1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to ground AC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="af2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ring to ground AC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bl1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Longitudinal balance in decibels</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="bl2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Capacitive balance in percent</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ct1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Dial tone test code (full only)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ct2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Dial tone test code (full only)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dc1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tree Terminal Tip to Ring DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dc2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tree Terminal Tip to ground DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dc3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tree Terminal Ring to ground DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dc4" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tree Terminal Tip to ground DC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dc5" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tree Terminal Ring to ground DC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dr1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Two Terminal Tip to Ring DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dr2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Two Terminal Tip to ground DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dr3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Two Terminal Ring to ground DC resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dr4" type="xs:string">
		<xs:annotation>
			<xs:documentation>Two Terminal Tip to ground DC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="dr5" type="xs:string">
		<xs:annotation>
			<xs:documentation>Two Terminal Ring to ground DC voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="lc1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Line circuit tip ground voltage </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="lc2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Line circuit ring ground voltage</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="rg1" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to ring ringers (count or Y)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="rg2" type="xs:string">
		<xs:annotation>
			<xs:documentation>Tip to ground ringers (count or Y)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="rg3" type="xs:string">
		<xs:annotation>
			<xs:documentation>Ring to ground ringers (count or Y)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk1" type="xs:string">
		<xs:annotation>
			<xs:documentation>1st T-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk10" type="xs:string">
		<xs:annotation>
			<xs:documentation>4th R-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk11" type="xs:string">
		<xs:annotation>
			<xs:documentation>5th R-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk12" type="xs:string">
		<xs:annotation>
			<xs:documentation>6th R-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk13" type="xs:string">
		<xs:annotation>
			<xs:documentation>Applied soak voltaje</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk2" type="xs:string">
		<xs:annotation>
			<xs:documentation>2nd T-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk3" type="xs:string">
		<xs:annotation>
			<xs:documentation>3rd T-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk4" type="xs:string">
		<xs:annotation>
			<xs:documentation>4th T-G soak resistance</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="sk5" type="xs:string">
		<xs:annotation>
			<xs:documentation>5th T-G soak resistance</xs:documentation>
