<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cm="http://www.telecom.com.ar/XMLSchema/bios/common" xmlns:rqm="http://www.telecom.com.ar/XMLSchema/bios/cliente/requestMessage">
	<!--xs:import namespace="http://www.telecom.com.ar/XMLSchema/bios/common" schemaLocation="../Commons/diccionario.xsd"/-->
	<xs:element name="oficinasComercialesDeUnClienteReq">
		<xs:complexType>
			<xs:choice>
				<xs:element name="crm">
					<xs:annotation>
						<xs:documentation>Identificación del Cliente en el CRM-Siebel</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="crmClienteID" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>ID del Cliente en CRM-Siebel</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="legado">
					<xs:annotation>
						<xs:documentation>Identificación del Cliente en el Legado/Comercial</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="legadoClienteID" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>ID del Cliente en el Legado</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>